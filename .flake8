[flake8]

# E402 module level import not at top of file
# E501 line too long (83 > 79 characters)
# F821 undefined name '_'

exclude = .git

max-line-length = 88

ignore =
    E203  # whitespace before ':' (Black)

per-file-ignores =
    ./i.sentinel_2.parallel.index/i.sentinel_2.parallel.index.py: E501,F821
